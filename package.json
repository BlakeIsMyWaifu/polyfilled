{
  "name": "polyfilled",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:cache-posts": "pnpm run build:1-contentlayer && pnpm run cache-posts",
    "dev:next": "next dev",
    "dev:browser": "pnpm run open-browser",
    "dev:contentlayer": "contentlayer dev",
    "build": "run-s build:*",
    "build:1-contentlayer": "contentlayer build",
    "build:2-cache-posts": "pnpm run cache-posts",
    "build:3-next": "next build",
    "start": "next start",
    "lint": "next lint",
    "open-browser": "start http://localhost:3000",
    "cache-posts": "node scripts/cachePostSlugs.mjs",
    "eslint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "analyze": "cross-env ANALYZE=true pnpm run build",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "ts-coverage": "typescript-coverage-report"
  },
  "dependencies": {
    "@octokit/auth-app": "^5.0.6",
    "@tanstack/react-query": "^4.29.15",
    "@trpc/client": "^10.32.0",
    "@trpc/next": "^10.32.0",
    "@trpc/react-query": "^10.32.0",
    "@trpc/server": "^10.32.0",
    "contentlayer": "^0.3.3",
    "next": "14.1.1",
    "next-contentlayer": "^0.3.3",
    "octokit": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.9.0",
    "react-is": "^18.2.0",
    "styled-components": "^5.3.10",
    "superjson": "^1.12.3",
    "zod": "^3.22.3",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.6",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/mdx": "^2.0.5",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@unchainedsky/eslint-config": "^1.0.0",
    "cross-env": "^7.0.3",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.6",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-pretty-code": "^0.9.11",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "typescript": "5.1.3",
    "typescript-coverage-report": "^0.7.0"
